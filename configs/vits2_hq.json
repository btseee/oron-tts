{
  "model_name": "vits2_mongolian_hq",
  "n_speakers": 2,
  "speaker_embedding_dim": 256,
  "audio": {
    "sample_rate": 22050,
    "n_fft": 1024,
    "n_mels": 80,
    "hop_length": 256,
    "win_length": 1024,
    "mel_fmin": 0.0,
    "mel_fmax": 8000.0
  },
  "text_encoder": {
    "n_vocab": 100,
    "hidden_channels": 256,
    "filter_channels": 1024,
    "n_heads": 4,
    "n_layers": 8,
    "kernel_size": 3,
    "dropout": 0.1
  },
  "posterior_encoder": {
    "in_channels": 80,
    "hidden_channels": 256,
    "out_channels": 256,
    "kernel_size": 5,
    "dilation_rate": 1,
    "n_layers": 20
  },
  "generator": {
    "initial_channel": 256,
    "resblock_type": "1",
    "resblock_kernel_sizes": [3, 7, 11],
    "resblock_dilation_sizes": [[1, 3, 5], [1, 3, 5], [1, 3, 5]],
    "upsample_rates": [8, 8, 2, 2],
    "upsample_initial_channel": 512,
    "upsample_kernel_sizes": [16, 16, 4, 4]
  },
  "discriminator": {
    "periods": [2, 3, 5, 7, 11],
    "use_spectral_norm": false
  },
  "flow": {
    "hidden_channels": 256,
    "kernel_size": 5,
    "dilation_rate": 1,
    "n_layers": 6,
    "n_flows": 6
  },
  "duration_predictor": {
    "hidden_channels": 256,
    "kernel_size": 3,
    "dropout": 0.5
  },
  "training": {
    "learning_rate": 0.0002,
    "betas": [0.8, 0.99],
    "eps": 1e-9,
    "lr_decay": 0.999875,
    "segment_size": 16384,
    "batch_size": 8,
    "epochs": 1000,
    "seed": 42,
    "fp16": true,
    "grad_clip": 1.0,
    "kl_weight": 1.0,
    "mel_weight": 45.0,
    "duration_weight": 1.0,
    "adversarial_weight": 1.0,
    "feature_matching_weight": 2.0
  }
}
